{
  "manifest_version": 2,

  "name": "Twitch.tv Dark Theme",
  "description": "",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://www.twitch.tv/*"],
      "css": ["mystyles.css"],
      "js": ["jquery-3.1.1.min.js", "myscript.js"]
    }
  ],
  "permissions": [
    "webNavigation",
    "https://www.twitch.tv/*"
  ]
}
